{
  "name": "expo-brownfield-target",
  "version": "0.5.0",
  "license": "MIT",
  "description": "Expo config plugin extending native projects for building app as a brownfield.",
  "keywords": [
    "react-native",
    "expo",
    "brownfield",
    "ios",
    "apple",
    "android"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": "./plugin/build/cli/index.js",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./package.json": "./package.json",
    "./app.plugin.js": "./app.plugin.js"
  },
  "files": [
    "android/",
    "!android/build/",
    "build/",
    "gradle-plugins/",
    "ios/",
    "plugin/",
    "src/",
    "LICENSE.txt",
    "README.md",
    "app.plugin.js",
    "expo-module.config.json",
    "package.json"
  ],
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "clean:plugin": "rm -rf plugin/build plugin/tsconfig.tsbuildinfo",
    "format:android": "./scripts/format-android.sh",
    "format:ios": "./scripts/format-ios.sh",
    "format:js": "prettier --write '**/*.{js,ts,tsx,json}'",
    "format": "npm-run-all format:*",
    "lint": "expo-module eslint",
    "lint:libOnly": "expo-module eslint --ignore-pattern 'example/*'",
    "lint:fix": "expo-module eslint --fix",
    "lint:fix:libOnly": "expo-module eslint --fix --ignore-pattern 'example/*'",
    "prepare": "husky && expo-module prepare && rm .eslintrc.js && chmod +x ./plugin/build/cli/index.js && cp -r ./plugin/src/templates ./plugin/build/templates",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "test": "jest",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion-labs/expo-brownfield-target.git"
  },
  "author": "Patryk Mleczek <patryk.mleczek@swmansion.com> (https://github.com/pmleczek)",
  "readmeFilename": "README.md",
  "homepage": "https://github.com/software-mansion-labs/expo-brownfield-target#readme",
  "bugs": {
    "url": "https://github.com/software-mansion-labs/expo-brownfield-target/issues"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "chalk": "^4.1.2",
    "ora": "^9.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "expo": "^54.0.18",
    "expo-module-scripts": "^5.0.7",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "expo": "*",
    "expo-build-properties": ">=1.0.0"
  }
}
