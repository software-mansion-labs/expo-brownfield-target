{
  "name": "expo-brownfield-target",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Expo config plugin extending native projects for building app as a brownfield.",
  "keywords": [
    "react-native",
    "expo",
    "brownfield",
    "ios",
    "apple",
    "android"
  ],
  "main": "./build/commonjs/index",
  "module": "./build/module/index",
  "types": "./build/typescript/index.d.ts",
  "source": "./plugin/index",
  "react-native": "./plugin/index",
  "bin": "./build/commonjs/cli/index.js",
  "exports": {
    ".": {
      "source": "./plugin/index.ts",
      "types": "./build/typescript/index.d.ts",
      "default": "./build/module/index.js"
    },
    "./package.json": "./package.json",
    "./app.plugin.js": "./app.plugin.js"
  },
  "files": [
    "app.plugin.js",
    "build/**/*",
    "plugin/**/*"
  ],
  "scripts": {
    "clean": "rimraf ./build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "npm run clean && bob build && chmod +x build/commonjs/cli/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion-labs/expo-brownfield-target.git"
  },
  "author": "Patryk Mleczek <patryk.mleczek@swmansion.com> (https://github.com/pmleczek)",
  "readmeFilename": "README.md",
  "homepage": "https://github.com/software-mansion-labs/expo-brownfield-target#readme",
  "bugs": {
    "url": "https://github.com/software-mansion-labs/expo-brownfield-target/issues"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "expo": "^54.0.18",
    "expo-build-properties": "^1.0.9",
    "expo-module-scripts": "^5.0.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react-native-builder-bob": "^0.40.13",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "expo": "*"
  },
  "react-native-builder-bob": {
    "source": "./plugin",
    "output": "./build",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "dependencies": {
    "chalk": "^5.6.2"
  }
}
